{
  "name": "vue-use-web",
  "version": "0.1.0",
  "description": "Web APIs implemented as Vue.js composition functions",
  "module": "dist/vue-use-web.esm.js",
  "unpkg": "dist/vue-use-web.js",
  "main": "dist/vue-use-web.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "docs:deploy": "./scripts/deploy.sh",
    "test": "jest",
    "test:cover": "jest --coverage",
    "lint": "eslint . '**/*.{js,jsx,ts,tsx}' --fix",
    "ts:defs": "./scripts/defs.sh",
    "build": "node ./scripts/build && npm run ts:defs"
  },
  "author": "Abdelrahman Awad <logaretm1@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@commitlint/cli": "^8.2.0",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "@typescript-eslint/parser": "^2.3.0",
    "@vue/composition-api": "^0.3.2",
    "@vue/test-utils": "^1.0.0-beta.29",
    "@vuepress/plugin-back-to-top": "^1.1.0",
    "@vuepress/plugin-google-analytics": "^1.1.0",
    "@vuepress/plugin-pwa": "^1.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.9.0",
    "bundlesize": "^0.18.0",
    "chalk": "^2.4.2",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.17.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "filesize": "^4.2.0",
    "flush-promises": "^1.0.2",
    "gzip-size": "^5.1.1",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "mkdirp": "^0.5.1",
    "prettier": "^1.18.2",
    "rollup": "^1.21.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-typescript2": "^0.24.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "uglify-js": "^3.6.0",
    "vue": "^2.6.10",
    "vue-jest": "^3.0.5",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --edit -E HUSKY_GIT_PARAMS"
    }
  },
  "bundlesize": [
    {
      "path": "./dist/*.min.js",
      "maxSize": "10 kB"
    }
  ],
  "eslintIgnore": [
    "locale",
    "dist",
    "scripts"
  ],
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add",
      "jest --maxWorkers=1 --bail --findRelatedTests"
    ],
    "*.js": [
      "git add"
    ]
  },
  "peerDependencies": {
    "@vue/composition-api": "^0.3.2"
  }
}
